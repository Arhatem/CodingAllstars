    var wait = 60;

    function time2(o, flag) {
        if (!flag && o['time_disabled']) {
            return;
        }
        var mobile = $("#mobile").val();
        if (mobile == "" || mobile == "请输入您的手机号") {
            alertShow("请输入您的手机号");
            return;
        }
        var phoneArea = $("#phoneArea").val();
        if (phoneArea == "") {
            phoneArea = "86";
        }
        phoneArea = phoneArea.replace("+", "");
        var areaTmp = phoneArea.replace("0", "");
        if (areaTmp == "86") {
            if (!(/^1[3-9][0-9]\d{8,11}$/.test(mobile))) {
                alertShow("手机号格式不正确");
                return;
            }
        }
        var captcha = document.getElementById("captcha").value.trim();
        if (captcha == "" || captcha == "请输入右侧验证码") {
            alertShow("请输入右侧验证码");
            return;
        }
        if (!flag) {
//        sendMobileCaptcha();

//        var mobile = $("#mobile").val();
//        var captcha = document.getElementById("captcha").value.trim();
            $.ajax({
                url: "https://icc.hep.com.cn:443/sendMobileCaptcha.action?time=" + new Date().getTime(),
                type: "post",
                data: {"mobile": mobile, "captchaType": 1, "inputRandomCode": captcha, "phoneArea":phoneArea},
                dataType: "json",
                success: function (data) {
                    if (data.result == true) {
                        if (wait == 0) {
                            o['time_disabled'] = false;
                            $(o).val("发送手机验证码");
                            wait = 60;
                        } else {
                            o['time_disabled'] = true;
                            $(o).val(wait + "秒后可以重新发送验证码");
                            wait--;
                            setTimeout(function () {
                                    time2(o, true);
                                },
                                1000);
                        }
                        alertShow("短信验证码已发送到您输入的手机号，请输入短信验证码。");
                    }
                    else {
                        reload_code();
                        alertShow(data.msg);

                    }
                }
            });
        }
        if (flag) {
            if (wait == 0) {
                o['time_disabled'] = false;
                $(o).val("发送手机验证码");
                wait = 60;
            } else {
                o['time_disabled'] = true;
                $(o).val(wait + "秒后可以重新发送验证码");
                wait--;
                setTimeout(function () {
                        time2(o, true);
                    },
                    1000);
            }
        }


    }

