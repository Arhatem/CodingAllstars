    function createNewUser() {
        var loginName = $.trim($("#ssoLoginName").val());
        if (!nameCheckFormat(loginName)) {
            alertLengthShow("用户名不能有空格且只能为英文或者数字，位数在6~20位之间!");
            return;
        }
        //密码
        var loginPassword = $("#ssoLoginPassword").val();
        var password = $("#ssoLoginPasswordConfirm").val();
        if (!passwordCheckFormat(loginPassword)) {
            alertLengthShow("密码不能有空格且只能为英文和数字，位数在8~20位之间!");
            return;
        }
        if (!passwordCheckFormat(password)) {
            alertLengthShow("密码不能有空格且只能为英文和数字，位数在8~20位之间!");
            return;
        }
        if (loginPassword != password) {
            alertShow("密码和确认密码不一致");
            return;
        }

        //注册协议
        if (!$("#checkId").is(":checked")) {
            alertLengthShow("请先仔细阅读此网站的《注册协议》，勾选同意后，再确认!");
            return;
        }
        var captcha = document.getElementById("reloadCheckImageSso").value.trim();
        if (captcha == ""||captcha == "请输入右侧验证码") {
            alertShow("请输入右侧验证码");
            return;
        }
        var flag = true;

        var token = $("#ssoToken").val();
        loginPassword = $.base64.btoa(loginPassword);
        // 验证唯一性
        $.ajax({
            url: "loginNameCheckAction.action",
            type: 'post',
            data: {loginName: loginName, inputRandomCode:captcha},
            async: false,
            success: function (data) {
                if (data == '-1') {
                    alertShow("验证码有误!");
                    return;
                }
                else if (data == '1') {
                    $.ajax({
                        url:"ssoLogin.action",
                        type:"post",
                        data:{"token": token, "loginUser.loginName":loginName, "loginUser.loginPassword":loginPassword, "inputRandomCodeSso":captcha},
                        dataType:"json",
                        success:function(data){
                            if (data.result == 1) {
                                window.location.href = "https://icc.hep.com.cn:443/intoMyCourses.action";
                            }
                            else {
                                alertShow(data.message);
                            }
                        }
                    });
                } else {
                    alertLengthShow("您注册的用户名已经被使用，请更换用户名注册!");
                    return ;
                }
            }
        });

	}
