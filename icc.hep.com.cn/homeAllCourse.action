            $("#captchaLogin2").show();
            $("#captchDiv").height(72);
            $("#loginFootDiv").css("margin-top", "67px");
        }
    }
    function submitLogin() {
        $("#submitButton").attr("disabled", true);
        var captcha = document.getElementById("captcha").value.trim();
        if (captcha == "" || captcha == "请输入右侧验证码") {
            confirmShow2("请输入右侧验证码", function() {$("#submitButton").attr("disabled", false);});
            return false;
        }
        if (loginType == 1) {
            var loginName = document.getElementById("loginName").value.trim();
            if (loginName == "" || loginName == "请输入您的账号或手机号") {
                confirmShow2("请输入您的账号", function() {$("#submitButton").attr("disabled", false);});
                return false;
            }
            var password = document.getElementById("loginPassword").value.trim();
            if (password == "") {
                confirmShow2("请输入您的密码", function() {$("#submitButton").attr("disabled", false);});
                return false;
            }

            //验证浏览器是否安装公式插件
            var ishaveEdit = 0;
            if (!$.browser.msie) {
                var num = navigator.plugins.length;
                for (i = 0; i < num; i++) {
                    //取得插件的名称
                    var name = navigator.plugins[i].name.toUpperCase();
                    if (name.startWith("MATHTYPE")) {
                        ishaveEdit = 1;
                        break;
                    }
                }
            } else {
                ishaveEdit = 1;
            }
            if (ishaveEdit == 1) {
                $("#ishaveEdit").val(1);
            } else {
                $("#ishaveEdit").val(0);
            }

            $("#loginUser_loginName").val(loginName);
//            $("#loginUser_loginPassword").val($.base64.btoa(password));
            $("#loginUser_loginPassword").val(encrypt.encrypt(password));

            $.ajax( {
                type : "post",
                url : "/checkInputRandomCode.action",
                data : {"inputRandomCode":captcha},
                dataType : "json",
                success : function(data) {
                    if(data.result == 0){
                        reload_code();
                        confirmShow2("验证码错误", function() {$("#submitButton").attr("disabled", false);});
                        return false;
                    }else {
                        $("#noLoginUserPnal").submit();
                    }
                }
            });

        }
        else {
            var mobile = document.getElementById("mobile").value.trim();
            if (mobile == "" || mobile == "请输入您的手机号") {
                confirmShow2("请输入您的手机号", function() {$("#submitButton").attr("disabled", false);});
                return false;
            }
            var phoneArea = $("#phoneArea").val();
            if (phoneArea == "") {
                phoneArea = "86";
            }
            phoneArea = phoneArea.replace("+", "").replace("0", "");
            if (phoneArea == "86") {
                if (!(/^1[3-9][0-9]\d{8,11}$/.test(mobile))) {
                    confirmShow2("手机号格式不正确", function() {$("#submitButton").attr("disabled", false);});
                    return false;
                }
            }
            var mobileCaptcha = document.getElementById("mobileCaptcha").value.trim();
            if (mobileCaptcha == "" || mobileCaptcha == "短信验证码") {
                confirmShow2("请输入短信验证码", function() {$("#submitButton").attr("disabled", false);});
                return false;
            }
            $("#mobileForm").val(mobile);
            $("#mobileCaptchaForm").val(mobileCaptcha);
            $("#captchaForm").val(captcha);
            $("#phoneAreaForm").val(phoneArea);
            jQuery.ajax( {
                type : "post",
                url : "/checkInputRandomCode.action",
                data : {"inputRandomCode":captcha},
                dataType : "json",
                success : function(data) {
                    if(data.result == 0){
                        reload_code();
                        confirmShow2("验证码错误", function() {$("#submitButton").attr("disabled", false);});
                        return false;
                    }else {
                        $("#captchaLoginForm").submit();
                    }
                }
            });

        }
    }

    function getCatcha() {
        reload_code();
        $("#captchaButton").hide();
        $("#captchaImage").show();
    }

    function getCookie(objName) {//获取指定名称的cookie的值
        var arrStr = document.cookie.split("; ");
        for (var i = 0; i < arrStr.length; i++) {
            var temp = arrStr[i].split("=");
            if (temp[0] == objName) return unescape(temp[1]);  //解码
        }
        return "";
    }

